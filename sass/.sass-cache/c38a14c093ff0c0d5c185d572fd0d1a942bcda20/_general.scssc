:@template"$/*------------------------------------*\
    MY MIXINS & SILENT CLASSES
\*------------------------------------*/
/**
* Attributes I will be using throughout the whole document.
*/

%title{ //title silent class, won't appear unless extended. Yours to define. 

}

%inherit-font{    //to avoid changes in font, mostly for form items
    font-family: inherit;
    font-size:   inherit;
    color: 		 inherit;
    font-weight: 300;
}

%erase-form-styling{           //to start styling from scratch on forms. User agent stylesheets are HELL.
    @extend %inherit-font;
    @include vendor(box-sizing, border-box);
    @include vendor(border-radius, 0);
    border: none;
    border-collapse: collapse;
}

@mixin z-index ($z-index-value){ //z-index is not supported in some devices unless you add the "transform" bit
    z-index: $z-index-value;
    -webkit-transform: translate3d(0,0,0);
}
%scroll-y{
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    overflow-x: hidden;
}
%scroll-x{
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    overflow-y: hidden;
}
%scroll-xy{
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

/*------------------------------------*\
    GENERAL SELECTORS
\*------------------------------------*/

* {
    @include vendor(box-sizing, border-box);   //When you set a size for a div, it counts the padding and border as part of that size.
}

html{
    height: 100%;
    width:      100%;
    -webkit-font-smoothing:  antialiased;
    -moz-osx-font-smoothing: grayscale;
    
}

body{
    height:     100%;
    width:      100%;
    //background: url('../images/background.jpg') no-repeat center center fixed;
    font-family: Avenir, Helvetica, Arial, sans-serif;
    background-size: cover;
    color: $color--foreground;

}

a{  //reset link styling (we don't want it underlined or its color changed unless WE tell it to)
    text-decoration: none;
    color: $color--highlight;
}

header{
    background-color: $color--background;
    padding: 50px 0 20px 0;
    text-align: center;
}
.logo--main{
    display: inline-block;
    height: 100px;
}
.nav--main{
    background-color: $color--highlight;
    color: $color--background;
    span{
        width: 25%;
        display: inline-block;
        text-align: center;
        padding: 6px 0;
        font-size: 1.2em;
        cursor: pointer;
    }
}

h1{
    margin: 0;
    padding-bottom: 20px;
    font-weight: normal;
    &:before{
        content: "";
        display: inline-block;
        width: 34px;
        height: 34px;
        background-image: url('../images/triangle.svg');
        background-size: contain;
        margin-right: 8px;
    }
}
h2{
    padding: 0 42px;
    margin: 0;
    font-weight: normal;
}
.content{
    width: 90%;
    max-width: 1000px;
    margin: 0 auto;
    position: relative;
}
.entry{
    transition: all 0.5s ease;
    background-color: rgba(255,255,255,0.9);
    width: 80%;
    height: 240px;
    max-width: 500px;
    margin: 0 auto;
    padding: 20px;
    border-right: 8px solid $color--highlight;
    img{
        position: absolute;
        width: 200px;
        transition: all 0.5s ease;
    }
    span{

    }
    ol{
        margin: 0;
        padding: 0;
        padding-left: 40px;
        padding-top: 10px;
        font-size: 1.2em;
    }
}
.entry__content{
    display: block;
    width: 100%;
    vertical-align: top;
    padding-left: 220px;
    h2{
        padding-left: 0;
        margin-bottom: -10px;
    }
}
.section--intro{
    background-color: $color--highlight;
    background-image: url('../images/background.jpg');
    background-size: 100% 100%;
    background-size: cover;
    padding: 40px 0;
    background-position: center center;
    background-repeat: no-repeat;
    background-attachment: fixed;
}
.section--about{
    background-color: $color--background;
    padding: 60px 0;
}
p{
    padding: 0;
    padding-left: 42px;
}
.skill__container{
    width: 100%;
    padding-right: 40px;
}
.skill{
    display: inline-block;
    width: 16.666666%;
    padding-left: 40px;
    transition: all 0.5s ease;
}


.section--work,
.section--more{
    padding-bottom: 100px;
    background-color: $color--background-2;
}

.section--work__title,
.section--more__title{
    padding-top: 60px;
    h1{
        width: 90%;
        max-width: 1000px;
        margin: 0 auto;
        @extend %js-animation;
    }
}
.section--work__title{
    background-color: $color--background-2;
}

.section--more{
    background-color: $color--background;
}

.more__grid{
    margin-left: -40px;
    .more__name{
        display: inline-block;
        width: 33.333%;
        padding-left: 40px;
    }
}
.more__name,
.project__name{
    cursor: pointer;
    transition: all 0.5s ease;
    @extend %js-animation;
    display: inline-block;
    width: 50%;
    position: relative;
    overflow: hidden;
    padding-left: 40px;
    margin-top: 40px;
    img{
        display: block;
    }
    span{
        text-align: right;
        font-size:1.2em;
        padding: 10px 15px;
        color: $color--background;
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: 2;
    }
    &:after{
        transition: all 0.5s ease;
        content:"";
        background-image: url('../images/triangle.svg');
        opacity: 0.9;
        background-size: cover;
        height: 100%;
        position: absolute;
        top: 0;
        bottom: 0;
        right: -100%;
        left: 0;
        z-index: 1;
    }
    &:hover:after,
    &.active:after{
        right: -1px;
    }
}

.project__name:after{
    background-image: url('../images/triangle-gray.svg');
    opacity: 0.4;
}
.project__name.project--abc span{
    color: darken($color-blue, 20);
}
.project__name.project--conp span{
    color: darken($color-orange, 20);
}
.project__name.project--tsys span{
    color: darken($color-red, 20);
}
.project__name.project--sisu span{
    color: darken($color-green, 20);
}
.project__name.project--juliuce span{
    color: darken($color-yellow, 20);
}
.project__name.project--production span{
    color: darken($color-purple, 20);
}
.work__grid{
    margin-left: -40px;
}

.section--contact{
    background-color: $color--highlight;
    background-image: url('../images/background.jpg');
    background-size: 100% 100%;
    background-size: cover;
    padding: 40px 0;
    background-position: center center;
    background-repeat: no-repeat;
    background-attachment: fixed;
    //background-color:rgba(168,201,0,0.5);
    span{
        padding: 20px 0;
        display: inline-block;
    }
    img{
        width: 25%;
        max-height: 160px;
        padding: 40px 10px;
        display: inline-block;
    }
}

footer{
    background-color: $color--background-2;
    text-align: right;
    .content{
        padding: 40px 0;
    }

}
.project__content,
.more__content{
    padding: 0;
    padding-bottom: 80px;
    background-color: $color--background-2;
    background-position: center center;
    background-repeat: no-repeat;
    background-attachment: fixed;
    img{
        max-height: 100%;
        max-height: 450px;
        margin: 40px auto;
        margin-bottom: 20px;
        display: block;
    }
}
.more__content{
    background-color: $color--background;
    p{
        margin: 0;
    }
    ul{
        margin-left: 60px;
        margin-bottom: 0;
    }
}

.grid--img{
    margin-left: -40px;
    img{
        display: inline-block;
        padding-left: 40px;
        width: 25%;
    }
}
.grid--img-3{
    @extend .grid--img;
    img{
        width: 33.3333%;
    }
}

.back{
    cursor: pointer;
    color: $color--highlight;
    &:before{
        content: "";
        background-image: url('../images/btn-more.svg');
        background-size: contain;
        width: 34px;
        display: inline-block;
        height: 34px;
        position: relative;
        top: 2px;
    }
}

.menu-float{
    position: fixed;
    background-color: $color--background-3;
    //color: $color--highlight;
    top: 10px;
    left: 10px;
    z-index: 10;
    -moz-box-shadow:2px 2px 4px rgba(0,0,0,0.1);
    -webkit-box-shadow:2px 2px 4px rgba(0,0,0,0.1);
    box-shadow:2px 2px 4px rgba(0,0,0,0.1);
    cursor: pointer;
    .menu__name{
        padding: 10px;
        display: block;
        span{
            display: block;
            background-image: url('../images/btn-menu.svg');
            background-repeat: no-repeat;
            background-position: right;
            background-size: contain;
            width: 100px;
            height: 30px;
            &:before{
                content:"";
                display: inline-block;
                background-image: url('../images/logoAM.svg');
                background-size: contain;
                width: 60px;
                height: 30px;
            }
        }
    }
    .menu__options{
        display: block;
    }
    .menu__options span{
        font-size: 1.2em;
        display: block;
        margin: 0;
        height: 50px;
        padding: 10px 15px;
        line-height: 30px;
        text-align: right;
        background-color: $color--background;
        &:nth-child(2n){
            background-color: $color--background-2;
        }
    }
